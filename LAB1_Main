package pl.lublin.wsei.java.cwiczenia;

public class Main {

    public static void main(String[] args) {

        System.out.printf("arg\t log2(arg)\t sum(arg)\n");

        double a=0;

        for(int i=0; i<10; i++){
            a=a+Math.pow(2,i);
            System.out.printf("%.0f\t %.0f\t %.0f\t\n", Math.pow(2,i), Math.log(Math.pow(2,i))/Math.log(2), a);
        }

    }
}
